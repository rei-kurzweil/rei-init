{
  "name": "@rei-init/cats",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/cats.js",
  "module": "./dist/cats.js",
  "types": "./dist/cats.d.ts",
  "exports": {
    ".": {
      "import": "./dist/cats.js",
      "types": "./dist/cats.d.ts"
    },
    "./spa": {
      "import": "./dist/cats-spa.js",
      "types": "./dist/cats-spa.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist/types && npm run build:exports",
    "build:exports": "node build-exports.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@rei-init/spa-auto-mount": "workspace:*",
    "@rei-init/ui": "workspace:*",
    "@react-three/fiber": "^9.3.0",
    "@react-three/xr": "^6.6.27",
    "@react-three/drei": "^9.92.7",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "three": "^0.160.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/three": "^0.160.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "vite": "^6.0.1"
  }
