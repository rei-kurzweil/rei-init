{
  "name": "@rei-init/meow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/meow.js",
  "module": "./dist/meow.js",
  "types": "./dist/meow.d.ts",
  "exports": {
    ".": {
      "import": "./dist/meow.js",
      "types": "./dist/meow.d.ts"
    },
    "./spa": {
      "import": "./dist/spa.js",
      "types": "./dist/spa.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist/types && npm run build:exports",
    "build:exports": "node build-exports.js",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@rei-init/spa-auto-mount": "workspace:*",
    "@rei-init/ui": "workspace:*",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.58.0",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.3",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.10.0",
    "vite": "^6.0.0"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  }
}