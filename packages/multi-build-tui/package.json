{
  "name": "@rei-init/multi-build-tui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "Interactive TUI to select which build targets to run for rei-cast-xyz",
  "bin": {
    "multi-build-tui": "dist/index.js",
    "multi-build-run": "dist/run.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/run.ts --format esm --dts",
    "dev": "tsup src/index.ts src/run.ts --watch --format esm --dts",
    "start": "bun run dist/index.js",
    "run": "bun run dist/run.js"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "colorette": "^2.0.20"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/node": "^22.9.0",
    "tsup": "^8.3.5",
    "typescript": "5.8.3"
  }
}