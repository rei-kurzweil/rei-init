{
  "name": "@rei-init/multi-build-tui",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "description": "Interactive TUI to select which build targets to run for rei-cast-xyz",
  "bin": {
    "multi-build-tui": "dist/index.js",
    "multi-build-run": "dist/run.js"
  },
  "scripts": {
  "build": "tsup src/index.tsx src/run.ts --dts",
  "dev": "tsup src/index.tsx src/run.ts --watch --dts",
    "start": "bun run dist/index.js",
    "run": "bun run dist/run.js"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "colorette": "^2.0.20",
  "react": "17.0.2",
    "react-blessed": "^0.7.2"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/node": "^22.9.0",
    "@types/react": "^19.0.1",
    "tsup": "^8.3.5",
    "typescript": "5.8.3"
  }
  ,"peerDependencies": {
    "react": ">=17.0.0"
  },
  "dependenciesMeta": {},
  "resolutions": {},
  "optionalDependencies": {
    "react-dom": "19.0.0"
  }
}