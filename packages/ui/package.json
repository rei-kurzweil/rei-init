{
    "name": "@rii-init/ui",
    "version": "0.1.0",
    "type": "module",
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "build:css": "postcss src/**/*.module.css --dir dist --ext .css",
        "build": "tsup src/index.ts && pnpm build:css",
        "dev": "tsup src/index.tsx --watch"
    },
    "devDependencies": {
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "postcss": "^8.5.6",
        "postcss-cli": "^11.0.1",
        "postcss-modules": "^6.0.1",
        "postcss-nested": "^7.0.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "tsup": "^8.5.0",
        "typescript": "^5.4.0"
    },
    "peerDependencies": {
        "react": ">=18.0.0 <20.0.0",
        "react-dom": ">=18.0.0 <20.0.0"
    }
}